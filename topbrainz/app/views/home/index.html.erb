<table class="table table-hover">
	<caption class="well"><strong>Homes table</strong></caption>
	<thead>
		<tr>
			<th>Title</th>
			<th>Category</th>
			<th>Bank balance</th>
			<th>Age</th>
			<th>Created</th>
		</tr>
	</thead>
	<tbody>
	<% @home.each do |home| %>
		<tr>
			<td><%= home.title %></td>
			<td><%= home.category %></td>
			<td><%= home.bank_blc %></td>
			<td><%= home.age %></td>
			<td><%= home.created_at %></td>
		</tr>
	<% end %>
	</tbody>
</table>

   
  

